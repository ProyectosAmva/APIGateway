{
  "Routes": [
    {
      "DownstreamPathTemplate": "/auth/token?username={username}&userType={userType}",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 7201
        }
      ],
      "UpstreamPathTemplate": "/api/token?username={username}&userType={userType}",
      "UpstreamHttpMethod": [ "Get" ]
    },
    {
      "AuthenticationOptions": {
        "AuthenticationProviderKey": "Bearer",
        "AllowedScopes": []
      },
      "RouteClaimsRequirement": {
        "UserType": "authorized"
      },
      "DownstreamPathTemplate": "/api/ExpedienteAmbiental/ObtenerExpedientesAmbientales",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 7012
        }
      ],
      "UpstreamPathTemplate": "/ObtenerExpedientesAmbientales",
      "UpstreamHttpMethod": [ "Get" ]
      
    }
  ],
  "GlobalConfiguration": {
    "BaseUrl": "https://localhost:5000"
  }
}
